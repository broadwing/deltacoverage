-- sampletestscript_test.go --
package sampletestscript_test

import (
	"sampletestscript"
	"testing"
)

func TestSumOnePlusOne(t *testing.T) {
	t.Parallel()

	want := 4
	got := sampletestscript.Sum(1, 1)
	if want != got {
		t.Errorf("want %d got %d", want, got)
	}
}

